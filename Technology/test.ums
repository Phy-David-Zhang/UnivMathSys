# Test of Set
print("\nTest of Set")
print(  "===========")

let X be Set
print(X)

A := {a\mid a\in A}
B := {b\in A\mid b\in B}
C := A\cup B
D := A\cap B
E := \complement_A B
F := A\times B
G := (A\cup B)\cap(C\cup D)\cap E

J := A\cup B\cup C
K := A\cap B\cap D

print(A, B)
print(B.Unique)
print(C, D)
print(E, F)
print(E.Property)
print(G)
print(J)
print(K)

# Test of Element
